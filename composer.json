  "license": "MIT",
  "support": {
    "docs": "https://merge.africa/blogs/custom-contact-form-integration/"
  },
  "authors": [
    {
      "name": "mergeTech",
      "homepage": "https://merge.africa/",
      "role": "software development company",
      "email": "info@mergemedia.co.za"
    }
  ],
  "require": {
    "illuminate/database": "~5.5.0|~5.6.0|~5.7|5.8.*",
    "illuminate/support": "~5.5.0|~5.6.0|~5.7|5.8.*",
    "buzz/laravel-google-captcha": "2.*"

  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "MergeTech\\ContactForm\\": "src"
    }
  },
  "extra": {
