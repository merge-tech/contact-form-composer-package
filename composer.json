{
  "name": "mergetech/contact-form",
  "keywords": [
    "laravel",
    "package",
    "contact",
    "contact form",
    "contact us",
    "get in touch",
    "merge_africa",
    "webdev",
    "php",
    "programming",
    "form",
    "email",
    "recaptcha"
